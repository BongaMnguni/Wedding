<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:orientation="vertical"
    android:padding="16dp"
    android:layout_height="wrap_content">

   <!-- <ImageView
        android:src="@drawable/icon_close"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" /> -->

    <LinearLayout
        android:id="@+id/dialog_couples"
        android:gravity="center"
        android:orientation="horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <ImageView
            android:layout_marginRight="20dp"
            android:src="@drawable/icons_couple"
            android:layout_width="50dp"
            android:layout_height="50dp" />

        <TextView
            android:textStyle="bold"
            android:textSize="17dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Couples"/>

    </LinearLayout>
    <TextView
        android:background="@drawable/underline"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <LinearLayout
        android:id="@+id/dialog_gen_images"
        android:gravity="center"
        android:layout_marginTop="16dp"
        android:orientation="horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <ImageView
            android:src="@drawable/icon_gallery"
            android:layout_marginRight="20dp"
            android:id="@+id/img"
            android:layout_width="50dp"
            android:layout_height="50dp" />

        <TextView
            android:textStyle="bold"
            android:textSize="17dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="General Images"/>

    </LinearLayout>
    <TextView
        android:background="@drawable/underline"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <LinearLayout
        android:id="@+id/dialog_dashboard"
        android:gravity="center"
        android:layout_marginTop="16dp"
        android:orientation="horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <ImageView
            android:src="@drawable/icon_dashboard"
            android:layout_marginRight="20dp"
            android:layout_width="50dp"
            android:layout_height="50dp" />

        <TextView
            android:textSize="17dp"
            android:textStyle="bold"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Dashboard"/>

    </LinearLayout>

</LinearLayout>
